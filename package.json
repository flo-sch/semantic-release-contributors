{
  "name": "semantic-release-contributors",
  "description": "semantic-release plugin to automatically update contributors list from git history",
  "version": "0.0.0-development",
  "author": "Florent Schildknecht <florent.schildknecht@gmail.com> (https://floschild.me)",
  "homepage": "https://github.com/flo-sch/semantic-release-contributors#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/flo-sch/semantic-release-contributors.git"
  },
  "bugs": {
    "url": "https://github.com/flo-sch/semantic-release-contributors/issues"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "keywords": [
    "author",
    "automation",
    "changelog",
    "commit",
    "contribution",
    "contributors",
    "conventional-changelog",
    "conventional-commits",
    "module",
    "package",
    "publish",
    "release",
    "semantic-release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "commit": "git-cz",
    "lint": "xo",
    "pretest": "yarn lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "dependencies": {
    "aggregate-error": "^3.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "parse-author": "^2.0.0",
    "stringify-author": "^0.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/error": "^2.2.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "ava": "^2.3.0",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "semantic-release": "^17.0.4",
    "sinon": "^7.4.1",
    "tempy": "^0.3.0",
    "xo": "^0.28.3"
  },
  "peerDependencies": {
    "semantic-release": ">=17.0.4 <18.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
